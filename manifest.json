{
    "manifest_version": 3,
    "name": "Normalize Google Search Line Height",
    "description": "Makes the highlight bar smaller",
    "version": "0.0.0.1",
    "icons": {
        "16": "/icons/icon16.png",
        "48": "/icons/icon48.png",
        "128": "/icons/icon128.png"
    },
    "content_scripts": [
        {
          "matches": ["https://*.google.com/*"],
          "css": ["styles.css"],
          "js": ["script.js"]
        }
      ]
}